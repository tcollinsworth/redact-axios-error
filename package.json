{
  "name": "redact-axios-error",
  "version": "0.1.3",
  "description": "grooms and redacts axios errors to trim them down and make them more useful for logging",
  "main": "index.js",
  "scripts": {
    "watchUnit": "NODE_ENV=DEBUG ava --fail-fast -v **/__tests__/**/*test*.js --watch",
    "ava": "ava",
    "test": "npm run ava",
    "build": "babel lib --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "watchLint": "esw . --ext=js --ignore-pattern '**/__tests__' --ignore-path .gitignore --fix --watch",
    "lint": "eslint . --ext=js --ignore-pattern '**/__tests__' --ignore-path .gitignore --fix"
  },
  "esm": {
    "force": true,
    "mode": "auto"
  },
  "keywords": [
    "redact",
    "error",
    "axios"
  ],
  "author": "Troy Collinsworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:tcollinsworth/redact-axios-error.git"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.4",
    "ignore-styles": "^5.0.1",
    "json-stringify-safe": "^5.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "esm": "^3.2.5",
    "lodash": "^4.17.11",
    "traverse": "^0.6.6"
  },
  "ava": {
    "files": [
      "**/__tests__/**/*test*.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "ignore-styles",
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
